{
  "name": "vue-phone-input",
  "version": "0.0.1",
  "description": "Phone number input with validation and location lookup",
  "author": "Brian Lee",
  "scripts": {
    "serve": "vue-cli-service serve --open --mode development demo/main.ts",
    "lint": "vue-cli-service lint",
    "build:demo": "vue-cli-service build src/main.ts",
    "build:lib": "vue-cli-service build --target lib --name vue-phone-input ./src/index.ts",
    "demo": "vue-cli-service serve --mode demo",
    "test:unit": "vue-cli-service test:unit",
    "ts:lib": "tsc -p src/tsconfig.json"
  },
  "dependencies": {
    "flag-icon-css": "^3.2.1",
    "libphonenumber-js": "^1.6.7",
    "vue": "^2.5.17",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0"
  },
  "devDependencies": {
    "@types/dom-inputevent": "^1.0.3",
    "@types/jest": "^23.1.4",
    "@types/node": "^10.12.10",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-typescript": "^3.1.1",
    "@vue/cli-plugin-unit-jest": "^3.1.1",
    "@vue/cli-service": "^3.1.4",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "dts-generator": "^2.1.0",
    "style-resources-loader": "^1.2.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^23.0.0",
    "typescript": "^3.0.0",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-template-compiler": "^2.5.17"
  },
  "files": [
    "src",
    "dist/*.js",
    "types/*.d.ts"
  ],
  "jsdelivr": "dist/vue-phone-input.js",
  "license": "MIT",
  "main": "dist/vue-phone-input.common.js",
  "module": "dist/vue-phone-input.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digital-drifter/vue-phone-input.git"
  },
  "typings": "types/index.d.ts",
  "unpkg": "dist/vue-phone-input.js"
}
